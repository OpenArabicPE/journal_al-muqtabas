                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">an advertisement</a:documentation>
                  <value>letter</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">a letter</a:documentation>
                  <value>verse</value>
