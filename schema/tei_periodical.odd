    <text>
        <front>
            <divGen type="toc"/>
        </front>
        <body>
            <p>This TEI customisation includes the modules:
                <list>
                    <item>core</item>
                    <item>tei</item>
                    <item>header</item>
                    <item>textstructure</item>
                    <item>tagdocs</item>
                    <item>namesdates</item>
                    <item>linking</item>
                    <item>figures</item>
                    <item>transcr</item>
                    <item>corpus</item></list>
            </p>
            <p>Many elements and attribute classes and attributes were deleted from the standard set. The values of some attributes were restricted.</p>
            <schemaSpec docLang="en" ident="tei_OpenArabicPE" prefix="OpenArabicPE_" xml:lang="en">
                <moduleRef except="addrLine address analytic binaryObject desc distinct divGen email emph gloss headItem headLabel index media meeting mentioned postBox postCode reg relatedItem series soCalled sp speaker stage street" key="core"/>
                <moduleRef except="" key="tei"/>
                <moduleRef except="abstract appInfo application cRefPattern catDesc catRef category classCode classDecl correction correspAction correspContext correspDesc creation extent geoDecl handNote hyphenation interpretation keywords listChange namespace normalization principal punctuation refState schemaRef scriptNote segmentation seriesStmt sponsor stdVals styleDefDecl tagUsage tagsDecl taxonomy textClass typeNote xenoData" key="header"/>
                <moduleRef except="argument div1 div2 div3 div4 div5 div6 div7 docAuthor docDate docEdition docImprint docTitle epigraph imprimatur postscript signed titlePage titlePart" key="textstructure"/>
                <moduleRef except="altIdent alternate anyElement attDef attList attRef classRef classSpec classes code constraint constraintSpec content dataFacet dataRef dataSpec datatype defaultVal eg egXML empty elementRef elementSpec equiv exemplum ident listRef macroRef macroSpec memberOf model modelGrp modelSequence moduleRef moduleSpec outputRendition param paramList paramSpec remarks schemaSpec sequence specDesc specGrp specGrpRef specList textNode val valDesc valItem valList" key="tagdocs"/>
                <!-- many elements should be removed from namesdates: sex socecStatus -->
                <moduleRef except="affiliation age bloc climate country district geogFeat geogName langKnowledge langKnown nationality offset  persona population region residence settlement sex socecStatus terrain" key="namesdates"/>
                <moduleRef except="alt altGrp anchor join joinGrp link linkGrp timeline when" key="linking"/>
                <moduleRef except="formula notatedMusic" key="figures"/>
                <moduleRef except="addSpan damage damageSpan delSpan handNotes handShift line listTranspose mod redo restore retrace secl sourceDoc space subst substJoin surplus transpose undo" key="transcr"/>
                <!-- corpus provides profileDesc, particDesc, settingDesc -->
                <moduleRef except="activity channel constitution derivation domain factuality interaction locale preparedness purpose setting textDesc" key="corpus"/>
                <!-- Changes to attribute classes -->
                <classSpec ident="att.breaking" mode="delete" module="tei" type="atts"/>
                <classSpec ident="att.canonical" mode="change" module="tei" type="atts">
                    <attList>
                        <attDef ident="key" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.coordinated" mode="delete" module="transcr" type="atts"/>
                <classSpec ident="att.damaged" mode="delete" module="tei" type="atts"/>
                <classSpec ident="att.datable" mode="change" module="tei" type="atts">
                    <attList>
                        <attDef ident="period" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.datable.custom" mode="change" module="namesdates" type="atts">
                    <attList>
                        <attDef ident="datingPoint" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.datable.iso" mode="delete" module="namesdates" type="atts"/>
                <classSpec ident="att.declarable" mode="delete" module="tei" type="atts"/>
                <classSpec ident="att.declaring" mode="delete" module="tei" type="atts"/>
                <classSpec ident="att.divLike" mode="change" module="tei" type="atts">
                    <attList>
                        <attDef ident="org" mode="delete"/>
                        <attDef ident="sample" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.dimensions" mode="change" module="tei" type="atts">
                    <attList>
                        <attDef ident="precision" mode="delete"/>
                        <attDef ident="scope" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.editLike" mode="delete" module="tei" type="atts"/>
                <classSpec ident="att.fragmentable" mode="delete" module="tei" type="atts"/>
                <classSpec ident="att.global" mode="change" module="tei" type="atts">
                    <attList>
                        <attDef ident="xml:base" mode="delete"/>
                        <attDef ident="xml:space" mode="delete"/>
                        <attDef ident="xml:lang" mode="change">
                            <defaultVal>ar</defaultVal>
                            <valList mode="replace" type="semi">
                                <valItem ident="ar">
                                    <desc>Arabic in Arabic script</desc>
                                </valItem>
                                <valItem ident="ota">
                                    <desc>Ottoman in Arabic script</desc>
                                </valItem>
                                <valItem ident="tr">
                                    <desc>Modern Turkish</desc>
                                </valItem>
                                <valItem ident="en">
                                    <desc>English</desc>
                                </valItem>
                                <valItem ident="fr">
                                    <desc>French</desc>
                                </valItem>
                                <valItem ident="de">
                                    <desc>German</desc>
                                </valItem>
                                <valItem ident="he">
                                    <desc>Hebrew in Hebrew script</desc>
                                </valItem>
                                <valItem ident="fa">
                                    <desc>Farsi in Arabic script</desc>
                                </valItem>
                                <valItem ident="ar-Latn-x-ijmes">
                                    <desc>Arabic in Latin script following the IJMES transcription</desc>
                                </valItem>
                                <valItem ident="ota-Latn-x-ijmes">
                                    <desc>Ottoman in Latin script following the IJMES transcription</desc>
                                </valItem>
                                <valItem ident="ota-Latn-TR">
                                    <desc>Ottoman in Latin script following common Turkish conventions</desc>
                                </valItem>
                                <valItem ident="fa-Latn-x-ijmes">
                                    <desc>Farsi in Latin script following the IJMES transcription</desc>
                                </valItem>
                                <valItem ident="ar-Latn-EN">
                                    <desc>Arabic in Latin script following common English conventions</desc>
                                </valItem>
                                <valItem ident="ar-Latn-FR">
                                    <desc>Arabic in Latin script following common French conventions</desc>
                                </valItem>
                                <valItem ident="ar-Latn-x-dmg">
                                    <desc>Arabic in Latin script following the DMG transcription</desc>
                                </valItem>
                                <valItem ident="de-Arab-AR">
                                    <desc>German transcribed into Arabic script following common Levantine Arabic practices</desc>
                                </valItem>
                                <valItem ident="en-Arab-AR">
                                    <desc>English transcribed into Arabic script following common Levantine Arabic practices</desc>
                                </valItem>
                                <valItem ident="fr-Arab-AR">
                                    <desc>French transcribed into Arabic script following common Levantine Arabic practices</desc>
                                </valItem>
                            </valList>
                        </attDef>
                    </attList>
                </classSpec>
                <classSpec ident="att.global.linking" mode="change" module="linking" type="atts">
                    <attList>
                        <attDef ident="copyOf" mode="delete"/>
                        <attDef ident="exclude" mode="delete"/>
                        <attDef ident="sameAs" mode="delete"/>
                        <attDef ident="select" mode="delete"/>
                        <attDef ident="synch" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.global.responsibility" mode="change" module="tei" type="atts">
                    <attList>
                        <attDef ident="cert" mode="delete"/>
                    </attList>
                </classSpec>
                <!-- we need @source at least in our authority files -->
                <!--<classSpec ident="att.global.source" mode="delete" module="tei" type="atts"/>-->
                <classSpec ident="att.media" mode="change" module="tei" type="atts">
                    <!-- even though I delete all attributes in this class, the class itself is required as it is member of att.internetMedia, which provides the @mimeType attribute -->
                    <attList>
                        <attDef ident="height" mode="delete"/>
                        <attDef ident="scale" mode="delete"/>
                        <attDef ident="width" mode="delete"/>
                    </attList>
                </classSpec>
                 <classSpec ident="att.naming" mode="change" module="tei" type="atts">
                    <attList>
                        <attDef ident="role" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.personal" mode="change" module="tei" type="atts">
                    <!-- even though I delete all attributes in this class, the class itself is required as it is member of att.canonical, which provides the @ref attribute -->
                    <attList>
                        <attDef ident="full" mode="delete"/>
                        <attDef ident="full" mode="delete"/>
                    </attList>
                </classSpec>
                 <classSpec ident="att.pointing" mode="change" module="tei" type="atts">
                    <attList>
                        <attDef ident="evaluate" mode="delete"/>
                        <attDef ident="targetLang" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.ranging" mode="delete" module="tei" type="atts"/>
                <classSpec ident="att.segLike" mode="delete" module="tei" type="atts"/>
                <classSpec ident="att.sortable" mode="delete" module="tei" type="atts"/>
                <classSpec ident="att.spanning" mode="delete" module="tei" type="atts"/>
                <classSpec ident="att.tableDecoration" mode="change" module="tei" type="atts">
                    <attList>
                        <attDef ident="role" mode="change" usage="opt">
                            <defaultVal>data</defaultVal>
                            <valList mode="replace" type="closed">
                                <valItem ident="data"/>
                                <valItem ident="label"/>
                            </valList>
                        </attDef>
                    </attList>
                </classSpec>
                <classSpec ident="att.timed" mode="change" module="tei" type="atts">
                    <attList>
                        <attDef ident="end" mode="delete"/>
                        <attDef ident="start" mode="delete"/>
                    </attList>
                </classSpec>
                <classSpec ident="att.transcriptional" mode="delete" module="tei" type="atts"/>
                <classSpec ident="att.written" mode="delete" module="tei" type="atts"/>
                <elementSpec ident="add" mode="change" module="core">
                    <attList>
                        <attDef ident="resp" mode="change" usage="req"/>
                    </attList>
                </elementSpec>
                <elementSpec ident="addName" mode="change" module="namesdates">
                    <attList>
                        <attDef ident="type" mode="change" usage="req">
                            <defaultVal>title</defaultVal>
                            <valList mode="replace" type="closed">
                                <!-- this is goind to be depreciated -->
                                <valItem ident="title">
                                    <desc>indicates formal titles, such as &quot;Bey&quot;, &quot;Pasha&quot; etc. This use is depreciated in favour of <tag>roleName type="title"</tag></desc>
                                </valItem>
                                <!-- this is goind to be depreciated -->
                                <valItem ident="honorific">
                                    <desc>indicates honorific addresses or titles, such as Miss, 
                                        <foreign xml:lang="ar-Latn-x-ijmes">Shaykh</foreign>
                                        , 
                                        <foreign xml:lang="ota-Latn-TR">zade</foreign>. This use is depreciated in favour of <tag>roleName type="honorific"</tag></desc>
                                </valItem>
                                <!-- this is goind to be depreciated -->
                                <valItem ident="rank">
                                    <desc>indicates ranks within an administrative, military or religious hierarchy, such as 
                                        <foreign xml:lang="ar-Latn-x-ijmes">farīq</foreign>
                                        , 
                                        <foreign xml:lang="ota-Latn-x-ijmes">vālī</foreign>
                                        , Captain. This use is depreciated in favour of <tag>roleName type="rank"</tag></desc>
                                </valItem>
                                <!-- this is goind to be depreciated -->
                                <valItem ident="nickname">
                                    <desc>a phrase someone is commonly referred to, such as &quot;Owner of the newspaper xyz&quot;. This use is depreciated in favour of <tag>roleName type="title"</tag></desc>
                                </valItem>
                                <!-- classic conception of Arabic names -->
                                <valItem ident="nasab">
                                    <desc>a 
                                        <term>patronym</term>
                                        introduced by “Ibn” (
                                        <foreign xml:lang="ar">إبن</foreign>
                                        - son of)</desc>
                                </valItem>
                                <valItem ident="kunyah">
                                    <desc>a 
                                        <term>teknonym</term>
                                        beginning with 
                                        <foreign xml:lang="ar-Latn-x-ijmes">Abū</foreign>
                                        (
                                        <foreign xml:lang="ar">أبو</foreign>
                                        - father of) or 
                                        <foreign xml:lang="ar-Latn-x-ijmes">Umm</foreign>
                                        (
                                        <foreign xml:lang="ar">أم</foreign>
                                        - mather of). It may also have hypothetical or metaphorical references as a mark of distinction. For example, 
                                        <foreign xml:lang="ar-Latn-x-ijmes">Abū Yūsuf</foreign>
                                        (
                                        <foreign xml:lang="ar">أبو يوسف</foreign>
                                        ) is often used for someone who is called 
                                        <foreign xml:lang="ar-Latn-x-ijmes">Ya'qūb</foreign>
                                        (
                                        <foreign xml:lang="ar">يعقوب</foreign>
                                        ) </desc>
                                </valItem>
                                <valItem ident="khitab">
                                    <desc>an honorific name, which is usually ended by the suffix 
                                        <foreign xml:lang="ar-Latn-x-ijmes">al-Dīn</foreign>
                                        (
                                        <foreign xml:lang="ar">الدين</foreign>
                                        ) </desc>
                                </valItem>
                                <valItem ident="nisbah">
                                    <desc>an adjective formed by using the suffix ī in order to indicate the person origin, his birth place, or his residence. It represents the relationship name, which can be a genealogical, political or ideological affiliation of a person. Example: the mathematician 
                                        <foreign xml:lang="ar-Latn-x-ijmes">al-Khawarizmī</foreign>
                                        refers to “Khuwarism”, the name of a place located in Central Asia</desc>
                                </valItem>
                            </valList>
                        </attDef>
                        <!-- subtype is depreciated -->
                        <attDef ident="subtype" mode="change" usage="opt">
                            <valList mode="replace" type="closed">
                                <valItem ident="military">
                                    <desc>Indicates a military rank, such as Captain or 
                                        <foreign xml:lang="ar-Latn-x-ijmes">Farīq</foreign></desc>
                                </valItem>
                                <valItem ident="civil">
                                    <desc>Indicates a civil rank within the bureaucracy</desc>
                                </valItem>
                                <valItem ident="religious">
                                    <desc>Indicates a religious rank, such as Bishop or 
                                        <foreign xml:lang="ar-Latn-x-ijmes">Muṭrān</foreign></desc>
                                </valItem>
                            </valList>
                        </attDef>
                    </attList>
                </elementSpec>
                <elementSpec ident="bibl" mode="change" module="core">
                    <attList>
                        <attDef ident="subtype" mode="change" usage="opt">
                            <valList mode="replace" type="semi">
                                <valItem ident="journal">
                                    <desc>A type of periodical publication that is called <foreign xml:lang="ar-Latn-x-ijmes">majalla</foreign> in Arabic.</desc>
                                </valItem>
                                <valItem ident="newspaper">
                                    <desc>A type of periodical publication that is called <foreign xml:lang="ar-Latn-x-ijmes">jarīda</foreign> in Arabic.</desc>
                                </valItem>
                            </valList>
                        </attDef>
                        <attDef ident="type" mode="change" usage="opt">
                            <valList mode="replace" type="semi">
                                <valItem ident="book">
                                    <desc>A book</desc>
                                </valItem>
                                <valItem ident="periodical">
                                    <desc>A periodical publication</desc>
                                </valItem>
                            </valList>
                        </attDef>
                        <attDef ident="frequency" mode="add" ns="https://openarabicpe.github.io/ns" usage="opt">
                            <desc>This attribute specifies the publication frequency</desc>
                            <datatype>
                                <dataRef key="teidata.text"/>
                            </datatype>
                            <defaultVal>monthly</defaultVal>
                            <valList mode="add" type="semi">
                                <valItem ident="daily"></valItem>
                                <valItem ident="weekly"></valItem>
                                <valItem ident="biweekly"></valItem>
                                <valItem ident="fortnightly"></valItem>
                                <valItem ident="monthly"></valItem>
                                <valItem ident="annually"></valItem>
                            </valList>
                        </attDef>
                    </attList>
                </elementSpec>
                <elementSpec ident="biblStruct" mode="change" module="core">
                   <attList>
                        <attDef ident="subtype" mode="change" usage="opt">
                            <valList mode="replace" type="semi">
                                <valItem ident="journal">
                                    <desc>A type of periodical publication that is called <foreign xml:lang="ar-Latn-x-ijmes">majalla</foreign> in Arabic.</desc>
                                </valItem>
                                <valItem ident="newspaper">
                                    <desc>A type of periodical publication that is called <foreign xml:lang="ar-Latn-x-ijmes">jarīda</foreign> in Arabic.</desc>
                                </valItem>
                            </valList>
                        </attDef>
                        <attDef ident="type" mode="change" usage="opt">
                            <valList mode="replace" type="semi">
                                <valItem ident="book">
                                    <desc>A book</desc>
                                </valItem>
                                <valItem ident="periodical">
                                    <desc>A periodical publication</desc>
                                </valItem>
                            </valList>
                        </attDef>
                        <attDef ident="frequency" mode="add" ns="https://openarabicpe.github.io/ns" usage="opt">
                            <desc>This attribute specifies the publication frequency</desc>
                            <datatype>
                                <dataRef key="teidata.text"/>
                            </datatype>
                            <defaultVal>monthly</defaultVal>
                            <valList mode="add" type="semi">
                                <valItem ident="daily"></valItem>
                                <valItem ident="weekly"></valItem>
                                <valItem ident="biweekly"></valItem>
                                <valItem ident="fortnightly"></valItem>
                                <valItem ident="monthly"></valItem>
                                <valItem ident="annually"></valItem>
                            </valList>
                        </attDef>
                    </attList>
                </elementSpec>
                <elementSpec ident="roleName" mode="change" module="namesdates">
                    <attList>
                        <attDef ident="type" mode="change" usage="req">
                            <defaultVal>title</defaultVal>
                            <valList mode="replace" type="closed">
                                <valItem ident="title">
                                    <desc>Formal titles, such as &quot;Bey&quot;, &quot;Pasha&quot; etc.</desc>
                                </valItem>
                                <valItem ident="honorific">
                                    <desc>Honorific addresses or titles, such as Miss, 
                                        <foreign xml:lang="ar-Latn-x-ijmes">Shaykh</foreign>
                                        , 
                                        <foreign xml:lang="ota-Latn-TR">zade</foreign></desc>
                                </valItem>
                                <valItem ident="nobility">
                                    <desc>An inherited or life-time title of nobility such as Lord, Viscount, Baron, etc.</desc>
                                </valItem>
                                <valItem ident="rank">
                                    <desc>indicates ranks within an administrative, military or religious hierarchy, such as 
                                        <foreign xml:lang="ar-Latn-x-ijmes">farīq</foreign>
                                        , 
                                        <foreign xml:lang="ota-Latn-x-ijmes">vālī</foreign>
                                        , Captain</desc>
                                </valItem>
                                <valItem ident="epithet">
                                    <desc>a phrase someone is commonly referred to (nick-name), such as &quot;Owner of the newspaper xyz&quot;</desc>
                                </valItem>
                                <valItem ident="pseudonym">
                                    <desc>a ficticious name, especially one used by an author.</desc>
                                </valItem>
                            </valList>
                        </attDef>
                        <attDef ident="subtype" mode="change" usage="opt">
                            <valList mode="replace" type="closed">
                                <valItem ident="military">
                                    <desc>Indicates a military rank, such as Captain or 
                                        <foreign xml:lang="ar-Latn-x-ijmes">Farīq</foreign></desc>
                                </valItem>
                                <valItem ident="civil">
                                    <desc>Indicates a civil rank within the bureaucracy</desc>
                                </valItem>
                                <valItem ident="religious">
                                    <desc>Indicates a religious rank, such as Bishop or 
                                        <foreign xml:lang="ar-Latn-x-ijmes">Muṭrān</foreign></desc>
                                </valItem>
                            </valList>
                        </attDef>
                    </attList>
                </elementSpec>
                <elementSpec ident="corr" mode="change" module="core">
                    <attList>
                        <attDef ident="resp" mode="change" usage="req"/>
                    </attList>
                </elementSpec>
                <elementSpec ident="date" mode="change" module="core">
                    <attList>
                        <attDef ident="calendar" mode="change">
                            <valList mode="replace" type="closed">
                                <valItem ident="#cal_gregorian">
                                    <desc>Gregorian calendar</desc>
                                </valItem>
                                <valItem ident="#cal_islamic">
                                    <desc>Muslim lunar calendar known as 
                                        <foreign xml:lang="ar-Latn-x-ijmes">hijrī</foreign> calendar
                                        beginning the year with 1 
                                        <foreign xml:lang="ar-Latn-x-ijmes">Muḥarram</foreign></desc>
                                </valItem>
                                <valItem ident="#cal_julian">
                                    <desc>Reformed Julian calendar also known as <foreign xml:lang="ar-Latn-x-ijmes">sharqī</foreign> or <foreign xml:lang="ar-Latn-x-ijmes">rūmī</foreign> calendar beginning the Year with 1 January</desc>
                                </valItem>
                                <valItem ident="#cal_ottomanfiscal">
                                    <desc>Ottoman fiscal calendar also known as 
                                        <foreign xml:lang="ar-Latn-x-ijmes">mālī</foreign>
                                        or 
                                        <foreign xml:lang="ar-Latn-x-ijmes">rūmī</foreign>
                                        calendar beginning the year with 1 <foreign xml:lang="ar-Latn-x-ijmes">mārt</foreign> (March), similar to the old Julian calendar.</desc>
                                </valItem>
                                <valItem ident="#cal_coptic">
                                    <desc>The Coptic calendar</desc>
                                </valItem>
                            </valList>
                        </attDef>
                        <attDef ident="datingMethod" mode="change">
                            <valList mode="replace" type="closed">
                                <valItem ident="#cal_gregorian">
                                    <desc>Gregorian calendar</desc>
                                </valItem>
                                <valItem ident="#cal_islamic">
                                    <desc>Muslim lunar calendar known as 
                                        <foreign xml:lang="ar-Latn-x-ijmes">hijrī</foreign> calendar
                                        beginning the year with 1 
                                        <foreign xml:lang="ar-Latn-x-ijmes">Muḥarram</foreign></desc>
                                </valItem>
                                <valItem ident="#cal_julian">
                                    <desc>Reformed Julian calendar also known as <foreign xml:lang="ar-Latn-x-ijmes">sharqī</foreign> or <foreign xml:lang="ar-Latn-x-ijmes">rūmī</foreign> calendar beginning the Year with 1 January</desc>
                                </valItem>
                                <valItem ident="#cal_ottomanfiscal">
                                    <desc>Ottoman fiscal calendar also known as 
                                        <foreign xml:lang="ar-Latn-x-ijmes">mālī</foreign>
                                        or 
                                        <foreign xml:lang="ar-Latn-x-ijmes">rūmī</foreign>
                                        calendar beginning the year with 1 <foreign xml:lang="ar-Latn-x-ijmes">mārt</foreign> (March), similar to the old Julian calendar.</desc>
                                </valItem>
                                <valItem ident="#cal_coptic">
                                    <desc>The Coptic calendar</desc>
                                </valItem>
                            </valList>
                        </attDef>
                        <attDef ident="type" mode="change">
                            <valList mode="replace" type="semi">
                                <valItem ident="official">
                                    <desc>A publication date as provided on the masthead</desc>
                                </valItem>
                                <valItem ident="scheduled">
