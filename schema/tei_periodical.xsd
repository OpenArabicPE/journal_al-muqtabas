               <xs:attributeGroup ref="tei:OpenArabicPE_att.global.linking.attribute.next"/>
               <xs:attributeGroup ref="tei:OpenArabicPE_att.global.linking.attribute.prev"/>
               <xs:attributeGroup ref="tei:OpenArabicPE_att.global.facs.attribute.facs"/>
               <xs:attributeGroup ref="tei:OpenArabicPE_att.global.change.attribute.change"/>
               <xs:attributeGroup ref="tei:OpenArabicPE_att.global.source.attribute.source"/>
               <xs:attributeGroup ref="tei:OpenArabicPE_att.placement.attributes"/>
               <xs:attributeGroup ref="tei:OpenArabicPE_att.typed.attributes"/>
               <xs:attributeGroup ref="tei:OpenArabicPE_att.dimensions.attributes"/>
               <xs:attribute name="resp" use="required">
                  <xs:annotation>
                     <xs:documentation>(responsible party) indicates the agency responsible for the intervention or interpretation, for example an editor or transcriber.</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                     <xs:restriction>
                        <xs:simpleType>
