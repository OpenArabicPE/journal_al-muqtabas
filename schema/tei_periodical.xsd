      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="tei:OpenArabicPE_macro.paraContent">
               <xs:attributeGroup ref="tei:OpenArabicPE_att.global.attribute.xmlid"/>
               <xs:attributeGroup ref="tei:OpenArabicPE_att.global.attribute.n"/>
               <xs:attributeGroup ref="tei:OpenArabicPE_att.global.attribute.xmllang"/>
