                  <xs:simpleType>
                     <xs:restriction>
                        <xs:simpleType>
                           <xs:list itemType="xs:anyURI"/>
                        </xs:simpleType>
                        <xs:minLength value="1"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
