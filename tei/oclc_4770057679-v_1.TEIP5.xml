                    <item change="#d2e341 #d2e389" xml:id="item_4.d2e279" xml:lang="en">مقالات المجلات</item>
                    <item change="#d2e341 #d2e389" xml:id="item_5.d2e281" xml:lang="en">مطبوعات ومخطوطات</item>
                    <item change="#d2e341 #d2e389" xml:id="item_6.d2e284" xml:lang="en">سير العلم والاجتماع</item>
                    <item change="#d2e341 #d2e389" xml:id="item_7.d2e286" xml:lang="en">أخبار وأفكار</item>
                </list>
            </change>
            <change when="2015-08-25" who="#pers_TG" change="#d2e341 #d2e389" xml:id="change_8.d2e290" xml:lang="en">Added <att change="#d2e341 #d2e389" xml:id="att_7.d2e292" xml:lang="en">type</att>="article" to all <gi change="#d2e341 #d2e389" xml:id="gi_13.d2e294" xml:lang="en">div</gi>s.</change>
            <change when="2015-08-25" who="#pers_TG" change="#d2e341 #d2e389" xml:id="change_9.d2e297" xml:lang="en">Converted the mark-up of <foreign xml:lang="ar-Latn-x-ijmes" change="#d2e341" xml:id="foreign_2.d2e299">qaṣīda</foreign>s from <gi change="#d2e341 #d2e389" xml:id="gi_14.d2e302" xml:lang="en">p</gi>s divided by a <gi change="#d2e341 #d2e389" xml:id="gi_15.d2e304" xml:lang="en">gap</gi> to <gi change="#d2e341 #d2e389" xml:id="gi_16.d2e306" xml:lang="en">lg</gi> of <att change="#d2e341 #d2e389" xml:id="att_8.d2e308" xml:lang="en">type</att>="bayt" comprising two <gi change="#d2e341 #d2e389" xml:id="gi_17.d2e311" xml:lang="en">l</gi> nodes.</change>
            <change when="2015-08-25" who="#pers_TG" change="#d2e341 #d2e389" xml:id="change_10.d2e314" xml:lang="en">Corrected the problem of trailing <gi change="#d2e341 #d2e389" xml:id="gi_18.d2e316" xml:lang="en">pb</gi>s by moving them to the beginning of the
